.list {
  width: 100%;
  height: 100%;
}

.header,
.footer {
  background-color: #333;
  line-height: 100rpx;
  color: #fff;
  text-align: center;
}

.item {
  display: flex;
  padding: 30rpx;
  line-height: 40rpx;
}

.item:nth-of-type(2n) {
  background-color: #6666ff;
}

.item:nth-of-type(2n + 1) {
  background-color: #ffff66;
}